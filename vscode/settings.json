  "editor.tabSize": 2,
  "editor.renderWhitespace": "boundary",
  "editor.wordWrap": "bounded",
  "editor.wrappingIndent": "indent",
  "flow.enabled": false,
  "search.useIgnoreFilesByDefault": true,
  "window.zoomLevel": 0,
  "workbench.iconTheme": "vs-seti",
