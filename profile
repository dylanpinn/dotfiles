#!/usr/bin/env sh

# Set vim as default editor.
export EDITOR="vim"

# Add Golang bin to PATH.
# export PATH=$PATH:$(go env GOPATH)/bin

# Add dotfiles bin to PATH
export PATH="$HOME/.bin:$PATH"

# Doom Emacs.
export PATH="$HOME/.emacs.d/bin:$PATH"

# Set NVM directory.
export NVM_DIR="$HOME/.nvm"

# vim: set syntax=bash:
